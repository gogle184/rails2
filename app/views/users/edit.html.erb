<h1>プロフィール編集</h1>

<%= form_with model:@user do |f|%>
 <table>
  <tr>
   <th><%= f.label :image,"画像"%></th>
   <td><%= f.file_field :image%></td>
  </tr>
  <tr>
   <th><%= f.label :name,"名前"%></th>
   <td><%= f.text_field :name %></td>
  </tr>  
  <tr>
   <th><%= f.label :profile,"自己紹介"%></th>
   <td><%= f.text_area :profile%></td>
  </tr>
 </table>

 <div>
  <ul>
   <li><%= f.submit "編集を完了する"%></li>
   <li><%= link_to"戻る",:back%></li>
  </ul>
 </div>


<%end%>